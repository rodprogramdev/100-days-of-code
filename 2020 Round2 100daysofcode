from flask import Flask

app = Flask(__name__)

@app.route('/<name>')
def index(name):
    return "<h1>HELLO {}!</h1>".format(name)

@app.route('/<string:name>')
def hello(name):
    return f"Hi, {name}!"



from flask import Flask, render_template

app = Flask(__name__)

@app.route("/")
def layout():
    return render_template("layout.html")


